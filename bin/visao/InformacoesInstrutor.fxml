<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="visao.InformacoesInstrutorController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" style="-fx-background-color: #201F1B;" VBox.vgrow="ALWAYS">
         <children>
            <Label layoutX="180.0" layoutY="29.0" text="EXIBINDO INFORMAÇÕES" textFill="#ff5e1e">
               <font>
                  <Font name="System Bold" size="23.0" />
               </font>
            </Label>
            <Label layoutX="169.0" layoutY="113.0" text="Matrícula:" textFill="#ff5e1e" />
            <TextField fx:id="fieldMatricula" disable="true" editable="false" layoutX="120.0" layoutY="139.0" />
            <Label layoutX="177.0" layoutY="192.0" text="Nome:" textFill="#ff5e1e" />
            <TextField fx:id="fieldNome" layoutX="120.0" layoutY="225.0" />
            <Label layoutX="431.0" layoutY="113.0" text="Email:" textFill="#ff5e1e" />
            <TextField fx:id="fieldEmail" layoutX="372.0" layoutY="139.0" />
            <TextField fx:id="fieldSenha" layoutX="372.0" layoutY="225.0" />
            <Label layoutX="429.0" layoutY="192.0" text="Senha:" textFill="#ff5e1e" />
            <Button fx:id="btnAtualizar" layoutX="342.0" layoutY="323.0" mnemonicParsing="false" onAction="#atualizarInfo" prefHeight="44.0" prefWidth="130.0" style="-fx-background-color: #ff5e1e;" text="ATUALIZAR" textAlignment="CENTER" textFill="WHITE" wrapText="true">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Button>
            <Button fx:id="btnVoltar" layoutX="173.0" layoutY="323.0" mnemonicParsing="false" onAction="#voltarTela" prefHeight="44.0" prefWidth="130.0" style="-fx-background-color: #ff5e1e;" text="VOLTAR" textAlignment="CENTER" textFill="WHITE" wrapText="true">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Button>
         </children>
    </AnchorPane>
  </children>
</VBox>
